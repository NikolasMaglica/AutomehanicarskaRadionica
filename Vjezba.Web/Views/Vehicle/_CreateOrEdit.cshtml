@model Vjezba.Model.Vehicle

<div class="form-group">
	<label class="control-label">Model</label>
	<input asp-for="ModelName" class="form-control" />
	<span asp-validation-for="ModelName" class="text-danger"></span>
</div>
<div class="form-group">
	<label class="control-label">Godina proizvodnje</label>
	<input asp-for="ModelYear" class="form-control" />
	<span asp-validation-for="ModelYear" class="text-danger"></span>
</div>

<div class="form-group">
	<label class="control-label">Proizvođač</label>
	<select asp-for="ManufacturerID" asp-items="ViewBag.PossibleVehicles" class="form-control"></select>
	<span asp-validation-for="ManufacturerID" class="text-danger"></span>
</div>

<div class="form-group" style="margin-top: 10px;">
	<input type="submit" value="Spremi" class="btn btn-outline-success" />
</div>

